	<appSettings/>
	<system.web.extensions>
		<scripting>
			<webServices>
				<jsonSerialization maxJsonLength="4097152">
					<!-- original was 2097152 -->
				</jsonSerialization>
			</webServices>
		</scripting>
	</system.web.extensions>
	<system.web>
		<httpCookies httpOnlyCookies="true"/>
		<pages viewStateEncryptionMode="Always" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
		<!--
          		<trace enabled="true" localOnly="true" pageOutput="false" requestLimit="1000" mostRecent="true" writeToDiagnosticsTrace="false" />
		-->
		<compilation debug="true" targetFramework="4.0">
			<assemblies>
				<!-- needed by O2 -->
				<add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
				<add assembly="System.DirectoryServices.AccountManagement, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
				<add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/></assemblies>
		</compilation>
		<authentication mode="None"/>
		<!-- this is actually a feature requirement for TeamMentor since the error messages are supposed to be handled by the client -->
		<customErrors mode="Off">
		</customErrors>
		<httpModules>
		</httpModules>
		<!--<trust level="Medium" />-->
		<!--<trust level="High" /> -->
		<httpRuntime maxRequestLength="50000" executionTimeout="3600"/>
	</system.web>
	<!-- 
        The system.webServer section is required for running ASP.NET AJAX under Internet
        Information Services 7.0.  It is not necessary for previous version of IIS.
    -->
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true">
